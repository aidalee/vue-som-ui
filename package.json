  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "less": "^4.1.2",
    "node-sass": "^7.0.1",
    "sass": "^1.52.3",
    "typescript": "^4.5.4",
    "vite": "^2.9.7",
    "vite-plugin-dts": "^1.1.1",
    "vite-plugin-inspect": "^0.5.0",
    "vite-plugin-md": "^0.11.0",
    "vitepress": "^0.22.3",
